{
  "name": "medusa-starter-default",
  "version": "0.0.1",
  "description": "E-Commerce Rental Platform",
  "author": "Juan Zapata Gomez <juanzgc@bu.edu>",
  "license": "MIT",
  "scripts": {
    "seed": "medusa seed -f ./data/seed.json",
    "build": "rm -rf dist && ./node_modules/.bin/tsc -p tsconfig.json",
    "start": "yarn run build && NODE_ENV=development node ./dist/main.js",
    "start:prod": "yarn run build && NODE_ENV=production node dist/main",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:create": "yarn run typeorm migration:create"
  },
  "dependencies": {
    "@medusajs/medusa": "^1.3.0",
    "@medusajs/medusa-cli": "^1.1.22",
    "lodash": "^4.17.21",
    "medusa-extender": "^1.6.1",
    "medusa-fulfillment-manual": "^1.1.26",
    "medusa-interfaces": "^1.1.27",
    "medusa-payment-manual": "^1.0.8",
    "medusa-payment-stripe": "^1.1.30",
    "mongoose": "^5.13.3",
    "typeorm": "0.2.36"
  },
  "repository": "https://github.com/medusajs/medusa-starter-default.git",
  "keywords": [
    "ecommerce",
    "rental",
    "headless",
    "medusa"
  ],
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-typescript": "^7.14.5",
    "babel-preset-medusa-package": "^1.1.13",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.5"
  }
}
